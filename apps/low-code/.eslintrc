{
  "env": {
    "browser": true,
    "es2020": true
  },
  "ignorePatterns": [
    "*.json"
  ],
  "extends": [
    "@antfu",
    "plugin:react-hooks/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react-refresh",
    "react"
  ],
  "rules": {
    "react-refresh/only-export-components": "warn",
    "no-console": "off",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}
